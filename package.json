{
  "name": "pomodoro-enforcer",
  "version": "1.0.0",
  "description": "An npm library that uses Pomodoro to make sure that you are taking enough breaks while coding.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "test": "jest --watchAll",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info"
  },
  "keywords": [
    "motivational",
    "productivity",
    "pomodoro",
    "efficiency"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neslihankara/pomodoro-enforcer.git"
  },
  "author": "Neslihan Kara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neslihankara/pomodoro-enforcer/issues"
  },
  "homepage": "https://github.com/neslihankara/pomodoro-enforcer#readme",
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "eqeqeq": "off"
    }
  }
